{
    "name": "deep-clean-obj",
    "version": "1.0.1",
    "description": "Recursive object cleaner that removes null, undefined, and empty strings",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "tsup",
        "test": "vitest run"
    },
    "keywords": [
        "clean",
        "object",
        "null",
        "undefined",
        "remove"
    ],
    "author": "BaryoDev",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/BaryoDev/BaryoDev.Libraries.JavaScript",
        "directory": "packages/deep-clean-obj"
    }
}
