{
    "name": "tiny-time-ago",
    "version": "1.0.0",
    "description": "Lightweight relative time formatting with locale support",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "tsup",
        "test": "vitest run"
    },
    "keywords": [
        "time-ago",
        "relative-time",
        "date",
        "format",
        "locale"
    ],
    "author": "BaryoDev",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/BaryoDev/BaryoDev.Libraries.JavaScript",
        "directory": "packages/tiny-time-ago"
    }
}